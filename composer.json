{
  "name": "rdanklof/custom-deliver-times",
  "description": "Custom Delivery Times for WooCommerce",
  "type": "project",
  "authors": [
    {
      "name": "Richard Danklof",
      "email": "richard@danklof.net"
    }
  ],
  "require": {
    "php": "^8.1",
    "monolog/monolog": "^2.2",
    "nesbot/carbon": "^2.45",
    "rachidlaasri/travel": "^1.0",
    "umulmrum/holiday": "^0.6.0"
  },
  "require-dev": {
    "phpro/grumphp": "^1.3",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "CustomDeliverTimes\\": "src/"
    }
  },
  "archive": {
    "exclude": [
      ".git",
      ".gitignore",
      ".phpunit.result.cache",
      "README.md",
      "grumphp.yml",
      "phpunit.xml",
      "tests",
      "!vendor/*",
      "*.zip"
    ]
  },
  "config": {
    "allow-plugins": {
      "phpro/grumphp": true
    }
  }
}
